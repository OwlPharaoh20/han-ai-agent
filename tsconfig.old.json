{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "outDir": "dist",
    "rootDir": ".",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": [
    "main.ts",
    "agent.ts",
    "models/*.ts",
    "tools/*.ts",
    "utils/*.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}